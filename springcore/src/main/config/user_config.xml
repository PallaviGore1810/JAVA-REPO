<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd
            http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context-3.0.xsd">
            <bean class="com.qspiders.springcore.entity.User" name="user1">
            <property name="id" value="1" ></property>
            <property name="userName" value="Akash"></property>
            <property name="email" value="akash@gmail.com"></property>
            <property name="password" value="Akash@123"></property>
            </bean>
            <bean class="com.qspiders.springcore.entity.User" name="user2">
            <property name="id" value="2" ></property>
            <property name="userName" value="Arti"></property>
            <property name="email" value="arti@gmail.com"></property>
            <property name="password" value="Arti@123"></property>
            </bean>
            <bean class="com.qspiders.springcore.entity.User" name="user3">
            <constructor-arg name="id" value="3" ></constructor-arg>
            <constructor-arg name="userName" value="Pallavi"></constructor-arg>
            <constructor-arg name="email" value="pallavi@gmail.com"></constructor-arg>
            <constructor-arg name="password" value="Pallavi@123"></constructor-arg>
            </bean>
</beans>